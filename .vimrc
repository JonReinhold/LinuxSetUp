if has('gui running')
  set guifont=Terminal:h9
 endif
 
colorscheme elflord
set lines=60 columns=108
set relativenumber
set gdefault
inoremap <C-1> <C-x><C-1>
syntax on
set synmaxcol=200
set wildmenu
set wildmode=full
set nowrap        
set tabstop=4     
set backspace=indent,eol,start                  
set autoindent    
set copyindent    
set shiftwidth=4  
set shiftround    
set showmatch     
set ignorecase    
set smartcase                     
set smarttab                  
set hlsearch      
set incsearch    
set showcmd
set backspace=indent,eol,start
set showcmd     

